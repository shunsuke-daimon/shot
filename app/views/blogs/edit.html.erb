<h1>Tweet編集</h1>

<%= form_with(model:@blog,local:true)do |form| %>

<div class ="tweet">
  <%= form.label :"Tweet内容" %>
  <%= form.text_field :content %>
</div>
<h1 class="btn btn-primary" role="button"><%= form.submit :"Tweetを変える"%></h1>
<h1 class="btn btn-primary" role="button"><%= link_to 'Tweet一覧にもどる', blogs_path %></h1>

<% end %>
